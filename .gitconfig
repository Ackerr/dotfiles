[alias]
    d = diff
    co = checkout
    br = branch
    brd = branch -D
    pod = push origin --delete
    st = status
    ci = commit
    cia = commit --all
    cim = commit --amend
    cima = commit --amend --all
    crp = cherry-pick
    dfl = diff --name-status
    dfs = diff --staged
    dl = diff HEAD^
    f = fetch --tags
    g = grep -in
    last = log -1 HEAD
    logg = log --graph --decorate --oneline --abbrev-commit --all
    logs = log --all --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    p = push
    pf = push -f
    puo = push -u origin
    puz = push -u z
    rb = rebase
    rbc = rebase --continue
    rbi = rebase -i
    rs = reset HEAD^
    rh = reset --hard
    rhd = reset --hard z/dev
    rroo = remote rename origin o
    squash = rebase HEAD~2 -i
    sl = stash list
    spop = stash pop
    sshow = stash show -p stash@{0}
[core]
    editor = vim
    autocrlf = false
    ignorecase = false
[color]
    ui = true
[merge]
    tool = vimdiff
